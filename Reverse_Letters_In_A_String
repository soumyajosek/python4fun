def revrev(to_rev):

    splitString=to_rev.split(' ')
    newString=""

    for str in splitString:
        count=len(str)-1
        while(count>=0):
            newString=newString+(str[count])
            count-=1
        newString=newString+(" ")
    print(''.join(newString))
   

revrev("The quick brown fox")
